                            android:background="@color/zxing_transparent"
                            android:orientation="horizontal">
                            <Button
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:text="Save All"
                                android:drawablePadding="5dp"
                                android:paddingTop="5dp"
                                android:textAllCaps="true"
                                android:id="@+id/saveAllId"
                                android:textColor="#FB3C3C"
                                android:textSize="18sp"
                                android:background="@color/zxing_transparent"
                                android:textColorHint="@color/lavenda"
                                android:textAppearance="@color/lavenda"
                                android:textStyle="bold"
                                android:textAlignment="textStart"
                                android:drawableLeft="@drawable/baseline_save_24"
                                android:layout_weight="0.5"
                                android:layout_marginBottom="0dp"/>
                            <Button
                                android:layout_width="60dp"
                                android:layout_height="40dp"
                                android:text="Save New"
                                android:drawablePadding="5dp"
                                android:paddingTop="5dp"
                                android:id="@+id/saveSpecificId"
                                android:textAlignment="textEnd"
                                android:textColor="#FB3C3C"
                                android:textSize="18sp"
                                android:textStyle="bold"
                                android:background="@color/zxing_transparent"
                                android:drawableRight="@drawable/baseline_add_24"
                                android:layout_weight="0.5"
